<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:card_view="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="20dp">
            <FrameLayout
                android:id="@+id/content"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" />
            <LinearLayout
                android:id="@+id/l0"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Test"
                android:orientation="horizontal"
                android:layout_marginTop="20dp">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="90dp">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@mipmap/jihuzur"/>
                </android.support.v7.widget.CardView>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/l1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Test"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/l0">
                <android.support.v7.widget.CardView
                    android:id="@+id/homeCleancardViewOne"
                    android:layout_width="140dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/homecleaning"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/homecleaning"/>

                    <TextView
                        android:id="@+id/homecleaningTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:text="@string/homecleaning"
                        android:textColor="@color/colorPrimary"
                        android:textSize="10sp"
                        android:textStyle="bold" />
                </android.support.v7.widget.CardView>
                <TextView
                    android:layout_width="30dp"
                    android:layout_height="100dp"
                    android:layout_margin="5dp"/>
                <android.support.v7.widget.CardView
                    android:id="@+id/cardViewOne1"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/massage"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/massage"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/massage"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"/>
                </android.support.v7.widget.CardView>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/l2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Test"
                android:orientation="horizontal"
                android:layout_below="@+id/l1"
                android:layout_marginTop="20dp">
                <android.support.v7.widget.CardView
                    android:id="@+id/cardViewTwo"
                    android:layout_width="140dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/moving"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/movingtruck"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/truck"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"/>
                </android.support.v7.widget.CardView>
                <TextView
                    android:layout_width="30dp"
                    android:layout_height="100dp"
                    android:layout_margin="5dp"/>
                <android.support.v7.widget.CardView
                    android:id="@+id/cardViewTwo1"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/ring"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/rings"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ring"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"/>
                </android.support.v7.widget.CardView>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/l3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Test"
                android:orientation="horizontal"
                android:layout_below="@+id/l2"
                android:layout_marginTop="20dp">
                <android.support.v7.widget.CardView
                    android:id="@+id/cardViewThree"
                    android:layout_width="140dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/teacher"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/teacher"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/teacher"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"/>
                </android.support.v7.widget.CardView>
                <TextView
                    android:layout_width="30dp"
                    android:layout_height="100dp"
                    android:layout_margin="5dp"/>
                <android.support.v7.widget.CardView
                    android:id="@+id/cardViewThree1"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/wasingMachine"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/washingmachine"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/washing"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"/>
                </android.support.v7.widget.CardView>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/l4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Test"
                android:orientation="horizontal"
                android:layout_below="@+id/l3"
                android:layout_marginTop="20dp">
                <android.support.v7.widget.CardView
                    android:id="@+id/cardViewThree11"
                    android:layout_width="140dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/teacher1"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/teacher"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/teacher"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"/>
                </android.support.v7.widget.CardView>
                <TextView
                    android:layout_width="30dp"
                    android:layout_height="100dp"
                    android:layout_margin="5dp"/>
                <android.support.v7.widget.CardView
                    android:id="@+id/cardViewThree111"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    card_view:contentPadding="16dp"
                    card_view:cardElevation="2dp"
                    card_view:cardCornerRadius="5dp">
                    <ImageView
                        android:id="@+id/wasingMachine1"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_horizontal"
                        android:src="@mipmap/washingmachine"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/washing"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"/>
                </android.support.v7.widget.CardView>

            </LinearLayout>




        </RelativeLayout>


    </LinearLayout>
</ScrollView>
